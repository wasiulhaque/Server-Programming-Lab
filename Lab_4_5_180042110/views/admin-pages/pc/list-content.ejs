<div class="right_col" role="main">
    <div class="col-md-12 col-sm-12">
      <div class="x_panel">
        <div class="x_title">
          <h2>Programming Contest<small>Team List</small></h2>
          <ul class="nav navbar-right panel_toolbox">
            <li>
              <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
          </ul>
          <div class="clearfix"></div>
        </div>
        <div class="x_content">
          <br />
  
          <% let success="Data has been deleted successfully!" %> <% let
          success2="Payment completed successfully!" %> <% let
          success3="Participant has been selected successfully!" %>
          <%if(error.length>0){if((error==success) || (error==success2) ||
          (error==success3)){%>
          <div class="alert alert-success"><%=error%></div>
          <%}else{%>
          <div class="alert alert-danger"><%=error%></div>
          <%}%> <%}%>
  
          <div class="row">
            <div class="col-sm-12" style="overflow: auto;">
              <div class="card-box table-responsive">
                <table
                  id="datatable-responsive"
                  class="table table-striped table-bordered dt-responsive nowrap"
                  cellspacing="0"
                  width="100%"
                >
                  <thead>
                    <tr>
                        <th class="table-bordered" colspan="2">Team Information</th>
                        <th class="table-bordered" colspan="2">Team Leader Information</th>
                        <th class="table-bordered" colspan="3">Team Member 1 Information</th>
                        <th class="table-bordered"  colspan="3">Team Member 2 Information</th>
                        <th class="table-bordered"  colspan="11">Status</th>
                    </tr>
                    <tr class="table-primary">
                        <th scope="col">Team ID</th>
                        <th scope="col">Coach</th>
    
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
    
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">T-Shirt</th>
    
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">T-Shirt</th>
    
                        <th scope="col">Selected</th>
                        <th scope="col">Payment <br> Status
                        </th>
                        <th scope="col">Select</th>
                        <th scope="col">Payment<br>Done</th>
                        <th scope="col">Edit</th>
                        <th scope="col">Delete</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if(team.length>0){
                    team.forEach(team=>{%>
                    <tr>
                        <td><%= team.teamHash %></td>
                        <td><%= team.coach %></td>

                        <td><%= team.leader %></td>
                        <td><%= team.emailLeader %></td>

                        <td><%= team.member1 %></td>
                        <td><%= team.emailMember1 %></td>
                        <td><%= team.tshirtMember1 %></td>

                        <td><%= team.member2 %></td>
                        <td><%= team.emailMember2 %></td>
                        <td><%= team.tshirtMember2 %></td>
                      
                      <td>
                        <% if(team.selected==true){%>
                        <p style="color: green">Yes</p>
                        <%}else{%>
                        <p style="color: red">No</p>
                        <%}%>
                      </td>
                      <td>
                        <% if(team.paid==team.total){%>
                        <p style="color: green">Successful</p>
                        <%}else{%>
                        <p style="color: red">Incomplete</p>
                        <%}%>
                      </td>
  
                      <td>
                        <% const selectURL="select/"+team._id%>
                        <a href="<%= selectURL %>">
                          <i class="fa fa-check"></i>
                          <span class="fa fa-chevron"></span>
                        </a>
                      </td>
                      <td>
                        <% const paymentDoneURL="paymentDone/"+team._id%>
                        <a href="<%= paymentDoneURL %>">
                          <i class="fa fa-money"></i>
                          <span class="fa fa-chevron"></span>
                        </a>
                      </td>
                      <td>
                        <% const editURL="edit/"+team._id%>
                        <a href="<%= editURL %>">
                          <i class="fa fa-pencil"></i>
                          <span class="fa fa-chevron"></span>
                        </a>
                      </td>
                      <td>
                        <% const deleteURL="delete/"+team._id%>
                        <a href="<%= deleteURL %>">
                          <i class="fa fa-trash"></i>
                          <span class="fa fa-chevron"></span>
                        </a>
                      </td>
                    </tr>
                    <% }) } %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  